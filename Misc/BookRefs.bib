@article{DeploymentLargeScaleWSNEnergyHole,
	Abstract = {Heterogeneous sensor networks have been proposed to address some fundamental limits and performance issues present in homogeneous Wireless Sensor Networks (WSNs). Questions such as the number of high-end sensors should be used, and how to deploy them, need proper assessment. In this work, we propose a novel model capable of representing a wide variety of scenarios, from totally random to planned stochastic node deployment in both homogeneous and heterogeneous sensor networks. In particular, this model encompasses networks with the characteristics of small-world networks. Using only about 3% of high-end sensors, and deploying nodes by using the slightly attractive model defined herein, we observe improved characteristics of the network topology, such as: (i) low average path length, (ii) high clustering coefficient, and (iii) improved relay task distribution among sensors. We also provide a guide for deploying nodes in order to improve the network lifetime, showing that the aforementioned model can be used to diminish the energy hole effect. Moreover, we evaluate a topological metric, namely Sink Betweenness, suitable for characterizing the relay task of a node.},
	Author = {H. S. Ramos and A. Boukerche and A. L. C. Oliveira and A. C. Frery and E. M. R. Oliveira and A. A. F. Loureiro},
	Doi = {10.1016/j.comnet.2016.09.013},
	File = {DeploymentLargeScaleWSNEnergyHole.pdf:rdt_pdf/DeploymentLargeScaleWSNEnergyHole.pdf:PDF},
	Journal = {Computer Networks},
	Keywords = {Wireless sensor networks; Network topology; Complex networks; Stochastic point process; Heterogeneous Wireless Sensor Networks},
	Pages = {154--167},
	Title = {On the Deployment of Large-Scale Wireless Sensor Networks Considering the Energy Hole Problem},
	Volume = {110},
	Year = {2016},
	Bdsk-Url-1 = {https://doi.org/10.1016/j.comnet.2016.09.013}}

