	Title = {Analytic Expressions for Stochastic Distances Between Relaxed Complex {W}ishart Distributions},
	Volume = {52},
	Year = {2014},
	Bdsk-Url-1 = {https://doi.org/10.1109/TGRS.2013.2248737}}

@article{FreryNascimentoCintraChileanJournalStatistics2011,
	Abstract = {This work presents a comprehensive examination of the use of information theory for understanding polarimetric synthetic aperture radar (PolSAR) images by means of contrast measures that can be used as test statistics. Due to the phenomenon called `speckle', common to all images obtained with coherent illumination such as PolSAR imagery, accurate modelling is required in their processing and analysis. The scaled multilook complex Wishart distribution has proven to be a successful approach for modelling radar backscatter from forest and pasture areas. Classification, segmentation, and im- age analysis techniques, which depend on this model, have been devised, and many of them employ some kind of dissimilarity measure. Specifically, we introduce statistical tests for analyzing contrast in such images. These tests are based on the Bhattacharyya, chi-square, Hellinger, Kullback-Leibler and R enyi distances. Results obtained by Monte Carlo experiments reveal the Kullback-Leibler distance as the best one with respect to the empirical test sizes under several situations which include pure and contaminated data. The proposed methodology is applied to actual data, obtained by an E-SAR sensor over surroundings of We{\ss}ling, Bavaria, Germany.},
	Author = {Frery, A C. and Nascimento, A. D. C. and Cintra, R. J.},
	File = {:/Users/acfrery/Library/texmf/bibtex/bib/rdt_pdf/FreryNascimentoCintraChileanJournalStatistics2011.pdf:PDF},
