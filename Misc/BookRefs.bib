@Article{GoodnessOfFitPolSARMellin,
  Title                    = {Goodness-of-Fit Tests for Multilook Polarimetric Radar Data Based on the {M}ellin Transform},
  Author                   = {Anfinsen, S.N. and Doulgeris, A.P. and Eltoft, T.},
  Journal                  = {IEEE Transactions on Geoscience and Remote Sensing},
  Pages                    = {2764--2781},
  Volume                   = {49},
  Year                     = {2011},
  Number                   = {7},

  Abstract                 = {The advent of polarimetric synthetic aperture radar has spurred a growing interest in statistical models for complex-valued covariance matrices, which is the common representation of multilook polarimetric radar images. In this paper, we respond to an emergent need by proposing statistical tests for the simple and composite goodness-of-fit (GoF) problem for a class of compound matrix distributions. The tests are based on Mellin-kind matrix cumulants. These are derived from a novel characteristic function for positive definite Hermitian random matrices, defined in terms of a matrix-variate Mellin transform instead of the conventional Fourier transform, and belong to a new framework for statistical analysis of multilook polarimetric radar data recently introduced by the authors. The cumulant-based tests are easy to compute, and the asymptotic sampling distribution of the test statistic is chi-square distributed in the simple hypothesis case. Under the composite hypothesis, the sampling distribution is obtained by Monte Carlo simulations. We evaluate the power of the proposed GoF tests with simulated data. We also use them to assess the fit of several matrix distributions to real data acquired by Radarsat-2 in fine-quad polarization mode.},
  Doi                      = {10.1109/TGRS.2010.2104158},
  File                     = {GoodnessOfFitPolSARMellin.pdf:art11_pdf/GoodnessOfFitPolSARMellin.pdf:PDF},
  ISSN                     = {0196-2892},
  Keywords                 = {Fourier transform;Hermitian random matrices;Mellin-kind matrix cumulant;Monte Carlo simulation;Radarsat-2;chi-square;complex-valued covariance matrices;compound matrix distribution;fine-quad polarization mode;goodness-of-fit test;matrix-variate Mellin transform;multilook polarimetric radar data;polarimetric synthetic aperture radar;statistical analysis;statistical model;Fourier transforms;Hermitian matrices;Monte Carlo methods;covariance matrices;polarimetry;random processes;statistical analysis;}
}

@Article{MellinAnalysisPolSAR,
  Title                    = {Application of the Matrix-Variate {M}ellin Transform to Analysis of Polarimetric Radar Images},
  Author                   = {Anfinsen, S.N. and Eltoft, T.},
  Journal                  = {IEEE Transactions on Geoscience and Remote Sensing},
  Pages                    = {2281--2295},
  Volume                   = {49},
  Year                     = {2011},
  Number                   = {6},

  Abstract                 = {In this paper, we propose to use a matrix-variate Mellin transform in the statistical analysis of multilook polarimetric radar data. The domain of the transform integral is the cone of complex positive definite matrices, which allows for transformation of the distributions used to model the polarimetric covariance and coherency matrix. Based on the matrix-variate Mellin transform, an alternative characteristic function is defined, from which we can retrieve a new kind of matrix log-moments and log-cumulants. It is demonstrated that the matrix log-cumulants are of great value to analysis of polarimetric radar data, and that they can be used to derive estimators for the distribution parameters with low bias and variance.},
  Doi                      = {10.1109/TGRS.2010.2103945},
  File                     = {MellinAnalysisPolSAR.pdf:art11_pdf/MellinAnalysisPolSAR.pdf:PDF},
  ISSN                     = {0196-2892},
  Keywords                 = {coherency matrix;complex positive definite matrices;distribution parameters;doubly stochastic product model;matrix log-cumulants;matrix log-moments;matrix-variate Mellin transform;matrix-variate statistics;multilook polarimetric radar data;polarimetric covariance;polarimetric radar images;statistical analysis;synthetic aperture radar;transform integral;geophysical image processing;geophysical techniques;radar polarimetry;statistical analysis;stochastic processes;synthetic aperture radar;}
}

@Article{TextureAnalysisGraphsSelfAvoidingWalks,
  author    = {Backes, A. R. and Martinez, A. S. and Bruno, O. M.},
