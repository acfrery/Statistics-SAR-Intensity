@Article{StatisticalAnalysisVHFBandTreeBackscattering,
  Title                    = {Statistical Analysis of {VHF}-Band Tree Backscattering Using Forest Ground Truth Data and {PO} Scattering Model},
  Author                   = {A. A. Kononov and A. Wyholt and G. Sandberg and L. M. H. Ulander},
  Journal                  = {IEEE Transactions on Geoscience and Remote Sensing},
  Pages                    = {3035--3046},
  Volume                   = {49},
  Year                     = {2011},
  Number                   = {8},

  Abstract                 = {This paper analyzes the statistical properties of the very high frequency (VHF)-band radar backscattering from coniferous trees by incorporating forest ground truth data into a physical-optics (PO) model that assumes horizontally transmit and receive polarizations and dominant double-bounce scattering from vertical stems standing on an undulating ground surface. The analysis shows that a statistically adequate model for the tree backscattering amplitude can be presented as a mixture of generalized gamma or lognormal distribution, and the mixture model can be reduced to a single density model if the trees with trunk volumes exceeding an appropriate threshold are to be taken into account. The generalized gamma density is shown to provide an appreciably better fit to the exceedance functions associated with the PO model data than that for the lognormal density. The results can be used to design statistically adequate models of forest clutter for VHF synthetic aperture radar systems.},
