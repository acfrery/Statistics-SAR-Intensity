@Article{HybridAdaptiveRoutingAlgorithmEvent-Driven,
  Title                    = {A Hybrid Adaptive Routing Algorithm for Event-Driven Wireless Sensor Networks},
  Author                   = {Figueiredo, C. M. S. and Nakamura, E. F. and Loureiro, A. A. F.},
