	Keywords = {G0 P mixture model;PolSAR data;SEM algorithm;image segmentation;moments method;multilook polarimetric SAR image clustering;random partition;sample quantile partition;stochastic expectation-maximization algorithm;expectation-maximisation algorithm;image segmentation;method of moments;radar imaging;radar polarimetry;stochastic processes;synthetic aperture radar;},
	Pages = {1--4},
	Title = {A comparison of clustering fully polarimetric {SAR} images using {SEM} algorithm and {G0P} mixture modelwith different initializations},
	Year = {2008},
	Bdsk-Url-1 = {https://doi.org/10.1109/ICPR.2008.4761244}}

@inproceedings{HortaMascaFreryIWSSIP2008,
